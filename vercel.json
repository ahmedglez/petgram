{
    "version": 2,
    "name": "petgram",
    "builds": [{ "use": "@vercel/static-build", "src": "package.json" }],
    "routes": [
        {
            "src":"/public/(.*)" ,
            "dest": "/public/$2"
        },
        {
            "src": "/(.*)",
            "dest": "vercel.json"
        },
        {
            "src": "/public/(.*)",
            "dest": "index.html"
        }
    ]
}
